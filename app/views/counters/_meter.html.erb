<%if @report %>
  <% @report.each do |r| %></div>
    <% date = DateTime.parse(r["created_at"]) %>
  <tr>
  	<% if  r["snapshot_url"] %>
      <td class="td_with_image"><%= link_to image_tag(@path+r["snapshot_url"]), @path+r["snapshot_url"], :class => "fancybox", :"data-fancybox-group" => "gallery" %></td>
    <% else %>
      <td>Нет картинки</td>
    <% end %>
    <td><%= r["user_id"].to_i %></td>
  	<td><%= r["reading"] %></td>
    <td><%= date.strftime("%Y-%m-%d") %></td>
  </tr>
  <% end %>
<% end %>